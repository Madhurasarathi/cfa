sudo ssh -i labsuser.pem ubuntu@<publicip>
sudo apt-get update -y
sudo apt-get install apache2 php libapache2-mod-php php-mysql mysql-client -y
sudo git clone <url>
cd cafedynamic/mompopcafe  ls
sudo cp -rf * /var/ww/html/
